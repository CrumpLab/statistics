j <- 0#
sub <- rep(FALSE, 1000)#
for(i in 1:dim(s)[1]){#
	if(sum((s[i,] - .5)^2) < .23){#
		j <- j+1#
		S[j,] <- s[i,]#
	}#
	if(sum((s[i,]-c(-0.15, 0.05)-.5)^2) < .115){#
		sub[j] <- TRUE#
	}#
}#
points(S, col=COL[1+sub], pch=20)#
text(0.5, 1, 'population of interest', pos=3)#
lines((x-0.5)*2*sqrt(0.115)+0.35,(y-.5)*2*sqrt(0.115)+0.55)#
#
