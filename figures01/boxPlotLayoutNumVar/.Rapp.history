data(email50)#
d   <- email50$num_char#
#
COL <- c('#22558888', '#664411EE')#
#
myPDF("boxPlotLayoutNumVar.pdf", 6, 4, mar=c(0,5,0,1), mgp=c(3.8, 0.7, 0), las=1)#
boxPlot(d, ylab='price ($1000s)', xlim=c(0.3,3), axes=FALSE, ylim=range(d)+sd(d)*c(-1,1)*0.2)#
axis(2)#
#
arrows(2,0, 1.35,min(d)-0.5, length=0.08)#
text(2,0,'lower whisker', pos=4)#
#
arrows(2, quantile(d, 0.25)+sd(d)/7, 1.35, quantile(d, 0.25), length=0.08)#
text(2, quantile(d, 0.25)+sd(d)/6.5, expression(Q[1]~~'(first quartile)'), pos=4)#
#
m <- median(d)#
arrows(2, m+sd(d)/5, 1.35, m, length=0.08)#
text(2,m+sd(d)/4.7,'median', pos=4)#
#
q <- quantile(d, 0.75)#
arrows(2, q+sd(d)/3, 1.35, q, length=0.08)#
text(2,q+sd(d)/2.8,expression(Q[3]~~'(third quartile)'), pos=4)#
#
arrows(2, rev(sort(d))[5]+sd(d)/4, 1.35, rev(sort(d))[5], length=0.08)#
text(2, rev(sort(d))[5]+sd(d)/3.9,'upper whisker', pos=4)#
#
y <- quantile(d, 0.75) + 1.5*IQR(d)#
arrows(2, y-0.1*sd(d), 1.35, y, length=0.08)#
lines(c(0.72,1.28), rep(y,2), lty=3, col='#00000066')#
text(2,y-0.1*sd(d),'max whisker reach', pos=4)#
#
m <- rev(tail(sort(d), 5))#
s <- m[1]-0.5*sd(m)#
arrows(2, s, 1.1, m[1]-200, length=0.08)#
arrows(2, s, 1.1, m[2]+300, length=0.08)#
arrows(2, s, 1.1, m[3]+350, length=0.08)#
arrows(2, s, 1.1, m[4]+400, length=0.08)#
text(2,s,'suspected outliers', pos=4)#
#
points(rep(0.4, 25), rev(sort(d))[1:25], cex=rep(1.3, 25), col=rep(COL[1], 25), pch=rep(1, 25))#
points(rep(0.4, 25), sort(d)[1:25], cex=rep(1, 25), col=rep(COL[2], 25), pch=rep("-", 25))#
