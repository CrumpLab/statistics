`dotPlot` <-#
function(x, vertical=FALSE, axes=TRUE, add=FALSE, ...){#
	y <- rep(1, length(x))#
	if(vertical){#
		if(add){#
			points(y,x,...)#
		} else {#
			plot(y,x,axes=FALSE,...)#
		}#
	} else {#
		if(add){#
			points(x,y,...)#
		} else {#
			plot(x,y,axes=FALSE,...)#
		}#
	}#
	if(axes){#
		axis(vertical+1)#
	}#
}
