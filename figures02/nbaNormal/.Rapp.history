library(openintro)#
data(COL)#
#
d <- read.delim('NBA-heights_08-09.txt',sep='\t')#
obs <- d[,4]#
#
hold <- hist(obs)#
#
myPDF("nbaNormal.pdf", mfrow=c(1,2), las=1, mgp=c(2,.7,0), mar=c(3,3,1,1))#
plot(0, 0, type='n', xlab='inches', ylab='', axes=F, main='', xlim=c(69, 90), ylim=c(0,.12))#
for(i in 1:length(hold$counts)){#
	rect(hold$breaks[i], 0, hold$breaks[i+1], hold$density[i],#
		 border='#222222', col='#BBCCDD')#
}#
axis(1)#
M  <- mean(obs)#
SD <- sd(obs)#
x <- seq(min(obs)-2, max(obs)+2, 0.01)#
y <- dnorm(x, M, SD)#
lines(x, y, col='#775533')#
#
qqnorm(obs, cex=0.5, main='', axes=F, ylab='NBA heights')#
axis(1)#
axis(2)#
abline(M, SD)#
#
dev.off()
